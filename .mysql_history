CREATE\040SCHEMA\040users
;
USE\040users
CREATE\040TABLE\040users\040(
\040\040id\040INT\040NOT\040NULL\040AUTO_INCREMENT\040PRIMARY\040KEY,
\040\040username\040VARCHAR(50)\040NOT\040NULL,
\040\040email\040VARCHAR(255)\040NOT\040NULL,
\040\040firstname\040VARCHAR(50)\040NOT\040NULL,
\040\040lastname\040VARCHAR(50)\040NOT\040NULL,
\040\040active\040BOOLEAN\040NOT\040NULL\040DEFAULT\040true
);
